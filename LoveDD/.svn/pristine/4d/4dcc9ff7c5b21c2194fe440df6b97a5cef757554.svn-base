@charset "UTF-8";
/*注册、登陆页面*/
.main {
    width: 100%;
    height: 500px;  
    background: url(../images/login_bg.png) 0 0 repeat-x;
}
.outer {
    margin: 0 auto;
    width: 1200px; 
    height: 500px;
    background: url(../images/signin_bg.jpg) -358px 0 no-repeat;
}
.sign {
    width: 850px;
    height: 436px; 
    padding: 64px 0 0 341px;
}
.sign h1 {
    width: 100%;
    height: 24px;
    padding: 22px 0;
    font-size: 24px;
    font-weight: bold;
    text-align: center;
}
.uselovedd {
    padding: 52px 0 0 0px;
    margin-left: 172px;
}
.sign h2 {
    float: left;
}
.sign h2 strong {
    float: left;
    padding: 0 20px 0 7px;
    border-left: 2px solid #000;
    font-size: 14px;   
}
.uselovedd h2 span {
    float: left;
    color: #c1c1c1;
}
.uselovedd h2 a {
    float: left;
    text-decoration: underline;
    color: #dc4935;
}
.uselovedd .write {
    float: left;
    width: 520px;
    margin-top: 28px;
}
.write input {
    float: left; 
    height: 32px;
    margin-right: 20px;
    padding-left: 10px;
    line-height: 32px;
    font-size: 16px;
    color: #c1c1c1;
    outline: none;
}
.write b {
    float: left;
    line-height: 36px;
    color: red;
}
/*input盖板 */
.write i {
    float: left;
    width: 34px;
    height: 30px;
    background: white;
    margin: 2px 0 0 -56px;
}
.tel, .keyword{
    width: 172px;
}
.confirm {
    width: 98px;
}
.write em {
    float: left;
    width: 72px;
    height: 24px;
    margin-top: 4px;
    margin-bottom: 14px;
    border-radius: 5px;
    border: 1px solid #d04936;
    line-height: 26px;
    text-align: center; 
    color: #fff;
    background-color: #f46352;
    cursor: pointer;
}
.agree {
    float: left;
    width: 350px;
    /*margin-top: 12px;*/
}
.agree .choose {
    float: left;
    width: 10px;
    height: 10px;
    /*margin-top: 3px;*/
    margin-right: 5px;
    border: 1px solid #c1c1c1;

}
.agree p {
    float: left;
    height: 14px;
    line-height: 14px;
    padding-top: 12px;
    margin-right: 20px;
    color: #c1c1c1;
    cursor: pointer;
}
.agree p span {
    float: left;
}
.agree p a {
    float: left;
    color: #dc4935;
}
.uselovedd .write .submit {
    float: left;
    width: 34px;
    height: 34px;
    background: url(../images/submits.png) 0 0 no-repeat;
    cursor: pointer;
}
.uselovedd .write .forget {
    float: left;
    height: 34px;
    margin-left: 7px;
    line-height: 34px;
    color: #c1c1c1;
}
.useother {
    width: 540px;
    height: 138px;
    margin-top: 20px;
    margin-left: 172px;
    padding-top: 20px;
    border-top: 1px dotted #c1c1c1;
}
.useother .otherup {
    float: left;
    width: 315px;
    margin-left: 42px;
}
.useother .otherup dl {
    float: left;
    height: 138px;
    margin-right: 25px;
}
.useother dl dt img {
    display: block;
    width: 80px;
    height: 80px;
}
.useother dl dd {
    width: 80px;
    margin-top: 15px;
}
.useother dl dd p {
    width: 100%;
    height: 12px;
    line-height: 12px;
    text-align: center;
    color: #c1c1c1;
}