.before-bro-sis-wlf, .now-bro-sis-wlf {
    overflow: hidden;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;   
}
.before-bro-sis-wlf, .family-wlf, .now-bro-sis-wlf {
    overflow: hidden;
    width: 100%;
    height: 100%;
}
.now-bro-sis-wlf {
    display: none;
}
.before-bro-sis-wlf img {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    display: block;
    width: 64.38%;
    height: 21.04rem;
    margin: auto;
}
@-webkit-keyframes Beforebrosis {
    from { 
        -webkit-transform: scale(1);
        transform: scale(1);
    }
    to { 
        -webkit-transform: scale(3);
        transform: scale(3);
    }
}
@-moz-keyframes Beforebrosis {
    from { 
        -webkit-transform: scale(1);
        transform: scale(1);
    }
    to { 
        -webkit-transform: scale(3);
        transform: scale(3);
    }
}
@-ms-keyframes Beforebrosis {
    from { 
        -webkit-transform: scale(1);
        transform: scale(1);
    }
    to { 
        -webkit-transform: scale(3);
        transform: scale(3);
    }
}
@-o-keyframes Beforebrosis {
    from { 
        -webkit-transform: scale(1);
        transform: scale(1);
    }
    to { 
        -webkit-transform: scale(3);
        transform: scale(3);
    }
}
@keyframes Beforebrosis {
    from { 
        -webkit-transform: scale(1);
        transform: scale(1);
    }
    to { 
        -webkit-transform: scale(3);
        transform: scale(3);
    }
}
