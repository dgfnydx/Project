.next-wlf {
    display: none;
    float: right;
    margin-right: 9.38%;
    width: 2.67rem;
    height: 2.67rem;
    -webkit-animation: nextPage 3s infinite;
       -moz-animation: nextPage 3s infinite;
        -ms-animation: nextPage 3s infinite;
         -o-animation: nextPage 3s infinite;
            animation: nextPage 3s infinite;
}
@-webkit-keyframes nextPage {
    0% { opacity : 1; }
    50% { opacity: 0.2;}
    100% { opacity : 1; }
}
@-moz-keyframes nextPage {
    0% { opacity : 1; }
