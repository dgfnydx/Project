/*基金详情页 涨跌幅*/
.wrap-fund-hyq .head h1,
.wrap-infm-hyq .head h1 {
    height: 2.75rem;
    padding-top: 0.58rem;
    line-height: 0;
}
.wrap-fund-hyq .head h1 strong,
.wrap-infm-hyq .head h1 strong {
    line-height: 1.5rem;
}
.wrap-fund-hyq .head h1 span,
.wrap-infm-hyq .head h1 span {
    font-size: 0.83rem;
    line-height: 1.25rem;
}
.main-hyq {
    padding-top: 3.33rem;
}
.overview-hyq {
    width: 97%;
    padding: 0.71rem 0 0.54rem 3%;
    background-color: #b20202;
}
.overview-hyq span {
    float: left;
    width: 22%;
    height: 2rem;
    margin-right: 2.8%;
    -webkit-border-radius: 0.21rem;
    -moz-border-radius: 0.21rem;
    -ms-border-radius: 0.21rem;
    border-radius: 0.21rem;
    color: #fff;
    font-size: 1rem;
    text-align: center;
    line-height: 2rem;
} 
.overview-hyq span:nth-child(1) {
    -webkit-box-shadow: 0 0 0 1px #fff;
    -moz-box-shadow: 0 0 0 1px #fff;
    -ms-box-shadow: 0 0 0 1px #fff;
    box-shadow: 0 0 0 1px #fff;
}
.fund-change {
    display: none; /*调试用看情况删除*/
}
.inform-hyq {
    width: 97%;
    padding: 0 1.5% 5.3rem 1.5%;
    background-color: #fff;
}
.title-fund {
    width: 100%;
    background-color: #eee;
    -webkit-box-shadow: 0 1px 0 0 #dfdfdf;
    -moz-box-shadow: 0 1px 0 0 #dfdfdf;
    -ms-box-shadow: 0 1px 0 0 #dfdfdf;
    box-shadow: 0 1px 0 0 #dfdfdf;  
}
.inform-hyq li {
    -webkit-box-shadow: 0 1px 0 0 #dfdfdf;
    -moz-box-shadow: 0 1px 0 0 #dfdfdf;
    -ms-box-shadow: 0 1px 0 0 #dfdfdf;
    box-shadow: 0 1px 0 0 #dfdfdf;   
}
.title-fund strong,
.inform-hyq li span {
    float: left;
    width: 25%;
    height: 3.33rem;
    color: #999;
    font-size: 1.17rem;
    text-align: center;
    line-height: 3.33rem;
}
.inform-hyq li span {
    height: 4rem;
    color: #333;
    line-height: 4rem;
    letter-spacing: -0.08rem;
}
.inform-hyq li span em {
    display: inline-block;
    width: 86%;
    height: 2.5rem;
    margin: 0.79rem auto 0;
    -webkit-border-radius: 0.17rem;
    -moz-border-radius: 0.17rem;
    -ms-border-radius: 0.17rem;
    border-radius: 0.17rem;
    color: #fff;
    text-align: center;
    line-height: 2.5rem;
    letter-spacing: 0;    
}

/*基金详情页 基金费率*/
.fund-rate {
    display: none; /*调试用看情况删除*/
    width: 96%;
    padding: 0.83rem 2% 5.3rem;
}
.manage-rate,
.buy-rate,
.redeem-rate,
.fixed-rate {
    width: 100%;
    margin-bottom: 1.25rem;
}
.fixed-rate {
    height: 2.96rem;
    background-color: #f3f3f3;
    -webkit-box-shadow:  0 0 0 1px #ccc;
    -moz-box-shadow:  0 0 0 1px #ccc;
    -ms-box-shadow:  0 0 0 1px #ccc;
    box-shadow:  0 0 0 1px #ccc;
    color: #333;
    font-size: 1.17rem;
    text-align: center;
    line-height: 2.96rem;
}
.manage-rate h1,
.buy-rate h1,
.redeem-rate h1 {
    width: 100%;
    height: 2.88rem;
    -webkit-box-shadow: 0 0 0 1px #ccc inset;
    -moz-box-shadow: 0 0 0 1px #ccc inset;
    -ms-box-shadow: 0 0 0 1px #ccc inset;
    box-shadow: 0 0 0 1px #ccc inset;
    background-color: #f3f3f3;
    color: #333;
    font-size: 1.17rem;
    text-align: center;
    line-height: 2.88rem;
}
.manage-rate p span,
.buy-rate p span,
.redeem-rate p span {
    float: left;
    width: 50%;
    height: 2.63rem;
    -webkit-box-shadow: 1px -1px 0 0 #ccc inset;
    -moz-box-shadow: 1px -1px 0 0 #ccc inset;
    -ms-box-shadow: 1px -1px 0 0 #ccc inset;
    box-shadow: 1px -1px 0 0 #ccc inset;
    color: #666;
    font-size: 1.17rem;
    text-align: center;
    line-height: 2.63rem;
}
.manage-rate p span:nth-child(2),
.buy-rate p span:nth-child(2),
.redeem-rate p span:nth-child(2) {
    -webkit-box-shadow: 1px -1px 0 0 #ccc inset, -1px 0 0 0 #ccc inset;
    -moz-box-shadow: 1px -1px 0 0 #ccc inset, -1px 0 0 0 #ccc inset;
    -ms-box-shadow: 1px -1px 0 0 #ccc inset, -1px 0 0 0 #ccc inset;
    box-shadow: 1px -1px 0 0 #ccc inset, -1px 0 0 0 #ccc inset;
}
.buy-rate .e-buy {
    width: 100%;
    height: 2.63rem;
    -webkit-box-shadow: 1px -1px 0 0 #ccc inset, -1px 0 0 0 #ccc inset;
    -moz-box-shadow: 1px -1px 0 0 #ccc inset, -1px 0 0 0 #ccc inset;
    -ms-box-shadow: 1px -1px 0 0 #ccc inset, -1px 0 0 0 #ccc inset;
    box-shadow: 1px -1px 0 0 #ccc inset, -1px 0 0 0 #ccc inset;
    color: #666;
    font-size: 1.17rem;
    text-align: center;
    line-height: 2.63rem;
}
.fund-rate .clarify {
    overflow: hidden;
    height: 5.75rem;
    padding: 0 3%;
    color: #535353;
    font-size: 1.17rem;
}

/*基金详情页 基金净值*/
.fund-worth {
    padding-bottom: 5.3rem;
    display: none; /*调试用看情况删除*/
}
.fund-chart {
    width: 100%;
}
#chartone {
    width: 100%;
    height: 10.67rem;
