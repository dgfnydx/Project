}
#charttwo {
    width: 100%;
    height: 8.5rem;
    margin-bottom: 2rem;
}
.chart-tab {
    width: 100%;
    -webkit-box-shadow: 0 1px 0 0 #dfdfdf inset;
    -moz-box-shadow: 0 1px 0 0 #dfdfdf inset;
    -ms-box-shadow: 0 1px 0 0 #dfdfdf inset;
    box-shadow: 0 1px 0 0 #dfdfdf inset;
}
.chart-tab span {
    float: left;
