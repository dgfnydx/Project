}
.inf-con {
    width: 96%;
    padding: 0 2%;
}
.inf-con li {
    width: 100%;
    box-shadow: 0 1px 0 0 #ccc;
}
.inf-con li span {
    height: 3.12rem;
    padding-top: 0.92rem;
    color: #666;
    line-height: 2.5rem;
    font-size: 1.42rem;
}
.inf-con li span:nth-child(2) {
    color: #d50000;
}
.inf-con li span em {
    display: block;
    width: 63%;
    height: 2.5rem;
    margin: 0 auto;
    -webkit-border-radius: 0.21rem;
    -moz-border-radius: 0.21rem;
    -ms-border-radius: 0.21rem;
    border-radius: 0.21rem;
    background-color: #d50000;
    color: #fff;
}
