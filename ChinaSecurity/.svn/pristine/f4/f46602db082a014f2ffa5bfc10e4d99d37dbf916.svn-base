/*注册*/
.dgf-main {
	background: white;
}
.login-tab {
	background: #eee;
	line-height: 3.29rem;
	font-size: 1.17rem;
}
.login-tab span {
	float: left;
	height: 3.29rem;	
	color: #999999;
}
.login-tab span:nth-child(1) {
	width: 42%;
	padding-right: 8%;
	text-align: right;
}
.login-tab .bac1 {
	background: url("../images/sanjiao.png") 74% bottom no-repeat;
	background-size: 8.13%;
	color: black;
}
.login-tab span:nth-child(2) {
	width: 45%;
	padding-left: 5%;
}
.login-tab .bac2 {
	background: url("../images/sanjiao.png") 26% bottom no-repeat;
	background-size: 8.13%;
	color: black;
}
.login-select {
	display: none;
}
.login {
	line-height: 3.25rem;
	padding: 1.96rem 6.25% 1.17rem 6.25%;
	color: #ccc;
}
.login .tabs div,
.logsame div {
	height: 3.25rem;
	margin-bottom: 0.83rem;
	font-size: 1.17rem;
}
.log-same input {
	display: block;
	width: 86.25%;
	height: 100%;
	padding-left: 13.75%;
	border: 0;
	outline: none;
	border-radius: 5px;
	box-shadow: 0 0 0 1px #ccc;
}
.log-dif input {
	float: left;
	width: 53.75%;
	height: 3.25rem;
	padding-left: 13.75%;
	border: 0;
	outline: none;
	border-radius: 5px;
	box-shadow: 0 0 0 1px #ccc;
}
.log-dif span {
	float: right;
	width: 30%;
	height: 3.25rem;
	color: #d20001;
	text-align: center;
	border-radius: 5px;
	box-shadow: 0 0 0 1px #d20001;
}
.log-user input {
	background: url("../images/user_name.png") 4.11% center no-repeat;
	background-size: 6.25%;
}
.log-mail input {
	background: url("../images/letter.png") 4.11% center no-repeat;
	background-size: 6.25%;
}
.log-tel input {
	background: url("../images/phone.png") 4.11% center no-repeat;
	background-size: 6.25%;
}
.log-pwd input,
.log-qpwd input {
	background: url("../images/lock.png") 4.11% center no-repeat;
	background-size: 6.25%;
}
.log-info {
	padding-left: 4.11%;
	background: url("../images/down.png") 90.89% center no-repeat;
	background-size: 4.11%;
	border-radius: 5px;
	box-shadow: 0 0 0 1px #ccc;
}
.enter-btn {
	display: block;
	height: 3.25rem;
	margin-top: 2.92rem;
	background: #d20001;
	color: white;
	font-size: 1.5rem;
	text-align: center;
	border-radius: 5px;
}
.log-count input {
	background: url("../images/count.png") 4.11% center no-repeat;
	background-size: 6.25%;
}
.log-id input {
	background: url("../images/id.png") 4.11% center no-repeat;
	background-size: 6.25%;
}
.log-dif input {
	background: url("../images/dif.png") 5.82% center no-repeat;
	background-size: 9.26%;
}
.login strong {
	display: block;
	height: 2rem;
	font-size: 1.17rem;
}
.login strong a {
	color: #ccc;
	text-decoration: underline;
}

/*登录*/
.enter-tips {
	height: 2.16rem;
	line-height: 2.16rem;
	color: red;
	font-size: 1rem;
}
.memory {
	padding-top: 1rem;
}
.memory b {
	float: left;
	width: 1.08rem;
	height: 1.08rem;
	line-height: 1.6rem;
	font-size: 1rem;
	text-align: center;
	box-shadow: 0 0 0 1px #ccc;
}
.memory em {
	float: left;
	margin-left: 3%;
	line-height: 1.08rem;
}
.memory a {
	float: right;
	line-height: 1.08rem;
}
.login-cover ul {
	overflow: hidden;
	width: 87.5%;
	margin: 18rem auto 0;
	padding: 0.58rem 0 0.17rem 0;
	background: #f5f5f5;
	border-radius: 5px;
}
.login-cover ul li {
	height: 3.63rem;
	padding-left: 12.14%;
	margin-bottom: -1px;
	line-height: 3.63rem;
	font-size: 1.17rem;
	box-shadow: 0 1px 0 0 #ccc;
}
/*密码重置*/
.reset-pwd {
	height: 3.33rem;
	background: #eee url("../images/sanjiao.png") center bottom no-repeat;
	background-size: 4.06%;
	line-height: 3.33rem;
	font-size: 1.17rem;
	text-align: center;
}
.sure-btn {
	margin-top: 4.3rem;
}
/*蒙版*/
.cover,
.login-cover {
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.2);
}
.log-win {
	width: 84.38%;
	margin: 14rem auto 0;
	background: #f5f5f5;
	font-size: 1.17rem;
	text-align: center;
	border-radius: 12px;
}
.log-win p {
	padding: 6.21rem 21% 2.29rem;
	background: url("../images/face.png") center 2.5rem no-repeat;
	background-size: 12.96%;
	box-shadow: 0 1px 0 0 #ccc;
}
.log-win span {
	display: block;
	padding: 1.04rem 0 1.17rem 0;
	color: #68a1fa;
}