.s-main {
    width: 100%;
    padding-top: 3.25rem;
    margin-bottom: 0.71rem;
    background-color: #f8f8f8;
    border-bottom: 1px solid #e9e9e9;
}
.s-pic {
    display: block;
    height: 7.68rem;
    padding-top: 4.79rem;
    background: url("../images/s_pic.jpg") 0 0 no-repeat;
    background-size: 100% 100%;
}
.s-pic h2 {
    margin-bottom: 0.5rem;
    padding-left: 8.13%;
    font-size: 1.5rem;
    color: white;
}
.s-contact {
    padding-top: 0.83rem;
    background-color: #fff;
}
.s-contact h2 {
    line-height: 5.17rem;
    text-align: center;
    color: #ffa751;
    font-size: 1.42rem;
}
.s-info {
    overflow: hidden;
    width: 56.41%;
    margin:  0 auto;
    
}
.s-info div {
    position: relative;
    width: 99%;
    height: 3.17rem;
    border: 1px solid #c7c7c7;
    border-radius: 0.17rem;
}
.s-info div .s-name {
    width: 81.72%;
    padding-left: 18.28%;
    background: #f5f5f5 url("../images/community.png") 0.5rem 0.75rem no-repeat;
    background-size: 1.71rem 1.71rem;
    font-size: 1.17rem;
    line-height: 3.17rem;
    outline: none;
    border: none;
}
.s-info em {
    visibility: hidden;
    font-size: 1.17rem;
    color: red;
}
.s-info div span {
    position: absolute;
    width: 10%;
    height: 3.17rem;
    right: 0;
    top: 0;
    background-color: #f5f5f5;
    border-radius: 0 0.17rem 0.17rem 0;
}
.s-info div .s-phone {
    width: 81.72%;
    padding-left: 18.28%;
    background: #f5f5f5 url("../images/phone.png") 0.92rem 0.67rem no-repeat;
    background-size: 0.96rem 1.83rem;
    font-size: 1.17rem;
    line-height: 3.17rem;
    outline: none;
    border: none;
}
.s-contact p {
    width: 56.41%;
    height: 3.17rem;
    margin: 0 auto;
    line-height: 3.17rem;
    text-align: center; 
    background-color: #ffa751;
    color: #fff;
    font-size: 1.42rem;
    border-radius: 0.17rem;
}
.s-contact a {
    display: block;
    margin: 0 auto;
    padding-left: 37.19%;
    line-height: 5.58rem;
    color: #ffa751;
    font-size: 1.42rem;
    font-style: italic;
    background: url("../images/tel.jpg") 31.09% 2.08rem no-repeat;
    background-size: 1.29rem 1.29rem;
}


