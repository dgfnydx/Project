/*loading*/
.pulladd {
    margin-top: 0.33rem;
    text-align: center;
    box-shadow: 0 -1px 1px #e9e9e9;
}
.pulladd span {
    display: block;
    width: 25%;
    padding-left: 4%;
    margin: 0 auto;
    color: #757575;
    font-size: 0.58rem;
    line-height: 2rem;
    background: url("../images/demo/p-icon.png") no-repeat left;
    background-size: 22%;
}
.addmore {
    display: none;
    height: 5.92rem;
    padding-top: 1.29rem;
    color: #4d4d4d;
    font-size: 0.88rem;
    text-align: center;
    background-color: #f8f8f8;
}
.addmore .loading {
    width: 13%;
    height: 3.33rem;
    margin: 0 auto 0.54rem;
    background: url("../images/ui-loading.png") no-repeat;
    background-size: 100%;
    -webkit-animation: circumgyrate 1s linear infinite;
}
@-webkit-keyframes "circumgyrate" {
    from {
            -webkit-transform: rotate(0deg);
        }
        to {
            -webkit-transform: rotate(360deg);
        }
}
