/*工地直播*/
.z-live-header {
    width: 100%;
    height: 3.125rem;
    position: fixed;
    top: 3.25rem;
    left: 0;
    background: #f8f8f8;
    z-index: 8;
    border-bottom: 1px solid #e9e9e9;
}

.z-live-header span {
    float: left;
    width: 33.333%;
    height: 100%;
    color: #4d4d4d;
    line-height: 3.125rem;
    text-align: center;
}
.z-live-header span i {
    display: inline-block;
    width: 1.2em;
    vertical-align: top;
    height: 100%;
    line-height: 100%;
    background: url("../images/tip1.png") center center no-repeat;
    background-size: 100%;
    margin-left: 6%;
    -webkit-transition: all 0.5s ease-in ;
    -moz-transition: all 0.5s ease-in ;
    -ms-transition: all 0.5s ease-in ;
    -o-transition: all 0.5s ease-in ;
    transition: all 0.5s ease-in ;
}
.z-live-cont {
    margin-top: 6.375rem ;
}
.z-live-cont a {
    float: left;
    width: 50%;
    height: 14.17rem;
    overflow: hidden;
    position: relative;
}
.z-live-cont a img {
    display: block;
    width: 100%;
    height: 100%;
}
.infos, .infos h3, .infos p {
    overflow:hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.infos h3 {
    margin-top: 0.67rem; 
}
.infos p {
